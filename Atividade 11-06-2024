#include <stdio.h>
int main()
{
   char codigo; //define as v치riaveis e o tipo.
   int qtd;
   float aPagar;
    printf("Digite o c칩digo:");
    scanf(" %c", &codigo);
    printf("Digite a Qtd vendida:");
    scanf("%d",&qtd);
    
    if(codigo == 'A' || codigo == 'a'){
        aPagar = qtd*10;
        
    }
    else if(codigo == 'B' || codigo == 'b'){
        aPagar = qtd*20;
       
        
    }else if(codigo == 'C' || codigo == 'c'){
        aPagar = qtd*30;
        
    }else{
        aPagar = 0;
        printf("C칩digo Errado");
    }
    if (aPagar > 0) { 
        printf("Valor pago ser치 %.2f",aPagar);
        if (qtd > 10){
            printf("\nForam vendidas mais de 10 unidade");
        }
    }
   
}
